        "arr = [5,1,8,7,2,4,6,3]\n",
        "n = 8\n",
        "\n",
        "def selectionSort(arr,n):\n",
        "  for i in range(n):\n",
        "    idx = i\n",
        "    for j in range(i+1,n):\n",
        "      if arr[j]<arr[idx]:\n",
        "        idx=j\n",
        "    arr[idx],arr[i]=arr[i],arr[idx]\n",
        "\n",
        "selectionSort(arr,n)\n",
        "print(arr)"
      ],
