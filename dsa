      "cell_type": "code",
      "source": [
        "# count number of connected components\n",
        "\n",
        "v = 10\n",
        "edges = [\n",
        "    [0,1],\n",
        "    [0,2],\n",
        "    [1,2],\n",
        "    [2,3],\n",
        "    [4,5],\n",
        "    [6,7],\n",
        "    [6,8],\n",
        "    [7,8],\n",
        "    [9,4]\n",
        "]\n",
        "\n",
        "adjList = buildAdjList(v,edges)\n",
        "\n",
        "visited = [False]*v\n",
        "ans = 0\n",
        "\n",
        "for i in range(v):\n",
        "  if not visited[i]:\n",
        "    dfs(adjList, i, visited)\n",
        "    ans+=1\n",
        "\n",
        "print(ans)"
