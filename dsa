        "\n",
        "# union\n",
        "print(set1|set2)\n",
        "print(set1.union(set2))\n",
        "\n",
        "# intersection\n",
        "print(set1&set2)\n",
        "print(set1.intersection(set2))\n",
        "\n",
        "# difference\n",
        "print(set1-set2)\n",
        "print(set1.difference(set2))\n",
        "# difference\n",
        "print(set2-set1)\n",
        "print(set2.difference(set1))\n",
        "\n",
        "# symmetric_difference = union-intersection\n",
