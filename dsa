    {
      "cell_type": "code",
      "source": [
        "def printLL(head):\n",
        "  curr = head\n",
        "  while curr!=None:\n",
        "    print(curr.data,end=\" \")\n",
        "    curr = curr.next\n",
        "  print()\n",
        "\n",
        "a = Node(5)\n",
        "b = Node(7)\n",
        "c = Node(2)\n",
        "a.next = b\n",
        "b.next = c\n",
        "head = a\n",
        "\n",
        "# insert at the beginning\n",
        "newNode = Node(4)\n",
        "newNode.next = head\n",
        "head = newNode\n",
        "\n",
        "# insert at the end\n",
        "newNode = Node(9)\n",
        "curr = head\n",
        "while curr.next!=None:\n",
        "  curr = curr.next\n",
        "curr.next = newNode\n",
        "\n",
        "# insert at kth index\n",
        "newNode = Node(6)\n",
        "k = 4\n",
        "curr = head\n",
        "for i in range(k-2):\n",
        "  curr = curr.next\n",
        "newNode.next = curr.next\n",
        "curr.next = newNode\n",
        "\n",
        "\n",
        "# remove the first node\n",
        "head = head.next\n",
        "\n",
        "# remove the last node\n",
        "curr = head\n",
        "while curr.next.next!=None:\n",
        "  curr = curr.next\n",
        "curr.next = None\n",
        "\n",
        "# remove the kth node\n",
        "k = 3\n",
        "curr = head\n",
        "for i in range(k-2):\n",
        "  curr = curr.next\n",
        "curr.next = curr.next.next\n",
        "\n",
        "\n",
        "printLL(head)"
      ],
