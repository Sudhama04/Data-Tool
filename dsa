        "v = 5\n",
        "egdes = [[0,1],[0,2],[2,4],[3,4],[1,4]]\n",
        "\n",
        "def buildAdjList(v, egdes):\n",
        "  adjList = [[] for i in range(v)]\n",
        "\n",
        "  for u,v in egdes:\n",
        "    adjList[u].append(v)\n",
        "    adjList[v].append(u)\n",
        "\n",
        "  return adjList\n",
        "\n",
        "\n",
        "adjList = buildAdjList(v, egdes)\n",
        "for i in range(v):\n",
        "  print(i,\"->\",adjList[i])"
