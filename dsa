    {
      "cell_type": "code",
      "source": [
        "arr = [5,1,8,7,2,4,6,3]\n",
        "n = 8\n",
        "\n",
        "def merge(arr,l,mid,r): # O(n)\n",
        "  A,B = arr[l:mid+1],arr[mid+1:r+1]\n",
        "  n,m = len(A),len(B)\n",
        "\n",
        "  i,j,k = 0,0,l\n",
        "  while i<n or j<m:\n",
        "    if j==m:\n",
        "      arr[k]=A[i]\n",
        "      i+=1\n",
        "      k+=1\n",
        "    elif i==n:\n",
        "      arr[k]=B[j]\n",
        "      j+=1\n",
        "      k+=1\n",
        "    elif A[i]<B[j]:\n",
        "      arr[k]=A[i]\n",
        "      i+=1\n",
        "      k+=1\n",
        "    else:\n",
        "      arr[k]=B[j]\n",
        "      j+=1\n",
        "      k+=1\n",
        "\n",
        "def mergeSort(arr,l,r): # O(nlogn)\n",
        "  if l<r:\n",
        "    mid = (l+r)//2\n",
        "    mergeSort(arr,l,mid)\n",
        "    mergeSort(arr,mid+1,r)\n",
        "    merge(arr,l,mid,r)\n",
        "\n",
        "\n",
        "mergeSort(arr,0,n-1)\n",
        "print(arr)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "GyJkvk1ToOcz",
        "outputId": "7abfacc0-344a-4539-9ccb-b9d946f28b44"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "[1, 2, 3, 4, 5, 6, 7, 8]\n"
          ]
        }
