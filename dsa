      "cell_type": "code",
      "source": [
        "arr = [3,2,3,4,5,6,5,4,3,1,2,3,4,5,6,0,5,6,4,0]\n",
        "n = len(arr)\n",
        "\n",
        "def countingSort(arr,n):\n",
        "  mx = max(arr)\n",
        "  freq = [0]*(mx+1)\n",
        "  for i in arr:\n",
        "    freq[i]+=1\n",
        "  arr.clear()\n",
        "  for i in range(mx+1):\n",
        "    while freq[i]>0:\n",
        "      arr.append(i)\n",
        "      freq[i]-=1\n",
        "\n",
        "countingSort(arr,n)\n",
        "print(arr)"
      ],
