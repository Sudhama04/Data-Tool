      "source": [
        "def fact(n):\n",
        "  # base case\n",
        "  if n==0 or n==1:\n",
        "    return 1\n",
        "  # recursive\n",
        "  return n*fact(n-1)\n",
        "\n",
        "print(fact(4))\n",
        "print(fact(6))"
      ],
      "metadata": {
