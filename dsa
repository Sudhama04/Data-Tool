      "cell_type": "code",
      "source": [
        "v = 5\n",
        "egdes = [[0,1],[0,2],[2,4],[3,4],[1,4]]\n",
        "\n",
        "def buildAdjMatrix(v, egdes):\n",
        "  adjMatrix = [[0 for j in range(v)] for i in range(v)]\n",
        "\n",
        "  for u,v in egdes:\n",
        "    adjMatrix[u][v] = 1\n",
        "    adjMatrix[v][u] = 1\n",
        "\n",
        "  return adjMatrix\n",
        "\n",
        "\n",
        "adjMatrix = buildAdjMatrix(v, egdes)\n",
        "for i in adjMatrix:\n",
        "  for j in i:\n",
        "    print(j,end=\" \")\n",
        "  print()"
      ],
