        "n = 5\n",
        "m = 3\n",
        "\n",
        "for i in range(n**2): # O(n^2)\n",
        "  print(i,end=\" \")\n",
        "\n",
        "for i in range(n): # O(n^3)\n",
        "  for j in range(n):\n",
        "    for k in range(n):\n",
        "      print(i,end=\" \")\n",
        "\n",
        "for i in range(n): # O(n^m)\n",
        "  for j in range(m):\n",
        "    print(i,j,end=\" \")"
