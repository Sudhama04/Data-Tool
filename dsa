        "from math import sqrt\n",
        "\n",
        "def isPrime(n):\n",
        "  if n==0 or n==1:\n",
        "    return False\n",
        "  for i in range(2,int(sqrt(n)+1)):\n",
        "    if n%i==0:\n",
        "      return False\n",
        "  return True\n",
        "\n",
        "print(isPrime(13))\n",
        "print(isPrime(20))"
      ],
