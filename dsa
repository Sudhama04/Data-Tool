      "cell_type": "code",
      "source": [
        "arr = [5,1,8,7,2,4,6,3]\n",
        "\n",
        "def linearSearch(arr,target):\n",
        "  for i in range(len(arr)):\n",
        "    if arr[i]==target:\n",
        "      return i\n",
        "  return -1\n",
        "\n",
        "print(linearSearch(arr,4))\n",
        "print(linearSearch(arr,5))\n",
        "print(linearSearch(arr,10))"
      ],
