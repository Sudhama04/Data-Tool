      "cell_type": "code",
      "source": [
        "for i in range(n): # O(n)\n",
        "  print(i,end=\" \")\n",
        "\n",
        "for i in range(n+100): # O(n)\n",
        "  print(i,end=\" \")\n",
        "\n",
        "for i in range(n-100): # O(n)\n",
        "  print(i,end=\" \")\n",
        "\n",
        "for i in range(n*100): # O(n)\n",
        "  print(i,end=\" \")\n",
        "\n",
        "for i in range(n//100): # O(n)\n",
        "  print(i,end=\" \")"
      ],
