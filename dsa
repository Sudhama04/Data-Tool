      "cell_type": "code",
      "source": [
        "class Stack:\n",
        "  def __init__(self):\n",
        "    self.val = []\n",
        "\n",
        "  def push(self,x):\n",
        "    self.val.append(x)\n",
        "\n",
        "  def pop(self):\n",
        "    self.val.pop()\n",
        "\n",
        "  def top(self):\n",
        "    if len(self.val)==0:\n",
        "      return -1\n",
        "    return self.val[-1]\n",
        "\n",
        "  def size(self):\n",
        "    return len(self.val)"
      ],
      "metadata": {
        "id": "CF654zk0IPiz"
      },
      "execution_count": null,
