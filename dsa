      "source": [
        "def gcd(a,b):\n",
        "  # base case\n",
        "  if b==0:\n",
        "    return a\n",
        "  # recursive\n",
        "  return gcd(b,a%b)\n",
        "\n",
        "print(gcd(21,70))\n",
        "print(gcd(60,24))"
      ],
