      "source": [
        "arr = [5,1,8,7,2,4,6,3]\n",
        "n = 8\n",
        "\n",
        "def bubbleSort(arr,n):\n",
        "  for i in range(n):\n",
        "    isSorted = True\n",
        "    for j in range(n-i-1):\n",
        "      if arr[j]>arr[j+1]:\n",
        "        arr[j],arr[j+1] = arr[j+1],arr[j]\n",
        "        isSorted = False\n",
        "    if isSorted:\n",
        "      break\n",
        "\n",
        "bubbleSort(arr,n)\n",
        "print(arr)"
      ],
      "metadata": {
