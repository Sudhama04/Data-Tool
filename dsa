        "arr = [5,1,8,7,2,4,6,3]\n",
        "n = 8\n",
        "\n",
        "def insertionSort(arr,n):\n",
        "  for i in range(1,n):\n",
        "    key = arr[i]\n",
        "    j = i-1\n",
        "    while j>=0:\n",
        "      if arr[j]<key:\n",
        "        break\n",
        "      arr[j+1]=arr[j]\n",
        "      j-=1\n",
        "    arr[j+1]=key\n",
        "\n",
        "insertionSort(arr,n)\n",
        "print(arr)"
      ],
