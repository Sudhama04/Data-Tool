    {
      "cell_type": "code",
      "source": [
        "def dfs(adjList, start, visited):\n",
        "  n = len(adjList)\n",
        "  ans = []\n",
        "  st = Stack()\n",
        "  ans.append(start)\n",
        "  visited[start]=True\n",
        "  st.push(start)\n",
        "\n",
        "  while st.size()>0:\n",
        "    top  = st.top()\n",
        "    st.pop()\n",
        "    for i in adjList[top]:\n",
        "      if not visited[i]:\n",
        "        ans.append(i)\n",
        "        visited[i]=True\n",
        "        st.push(i)\n",
        "\n",
        "  return ans"
      ],
      "metadata": {
        "id": "XBT9UuAj4NlA"
      },
      "execution_count": null,
      "outputs": []
    },
